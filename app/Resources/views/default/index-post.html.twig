{% extends 'base.html.twig' %}
{% block title %} All Posts {% endblock %}
{% block body %}



    {% for post in posts %}


        <div class="container">
            <div class="jumbotron">
                <a href="{{ path('view_post', {'slug':post.slug }) }}"><h3>{{ post.title }}</h3></a>
                <p>{{ post.text }}</p>
                <p><a class="btn btn-primary btn-lg" href="{{ path('view_post', {'slug':post.slug }) }}" role="button">Learn more</a></p>
            </div>
        </div>

    {% endfor %}

{% endblock %}